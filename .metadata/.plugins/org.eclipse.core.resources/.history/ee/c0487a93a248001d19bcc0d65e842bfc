package starprinting;

public class Star1 {

	public static void main(String[] args){
		 int k=1;
		 int n<10;
		for(int i=1; i<=5; i++) {//row
			for(int j=1; j<5;j++) { //space
				//if(i==0 && j==1)
				System.out.print(k+"  ");
				k++;
				System.out.println("0");
			
			
			}
			System.out.println( ); 
			
		}
	}
}
			
	


