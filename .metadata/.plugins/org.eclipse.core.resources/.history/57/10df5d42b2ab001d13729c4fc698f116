package String;

public class ReverseString {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String S="GeeksforGeeks";
		 int temp=0;
	        int i=0;
	        int e=S.length()-1;
	        while(i<e){
	            temp=S.charAt(i);
	            S.charAt(i)=S.charAt(e);
	            S.charAt(e)=temp;
	            i++;
	            e--;
	}

}
