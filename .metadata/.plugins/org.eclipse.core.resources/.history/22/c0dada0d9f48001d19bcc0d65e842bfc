package starprinting;

public class star3 {
	public static void main(String[] args) {
		for(int i=0; i<5; i++) {//row
			for(int j=i; j<4;j++) { //space
				//if(i==0 && j==1)
				System.out.print(" ");

			}
			for(int k=0;k<=i;k++) {
			System.out.print("*");
			}
			System.out.println();
		}
	}

}
